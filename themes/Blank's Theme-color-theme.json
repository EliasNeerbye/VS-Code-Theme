{
    "name": "Blank's Theme",
    "colors": {
        "editor.background": "#000010",
        "sideBar.background": "#000010",
        "activityBar.background": "#000129",
        "statusBar.background": "#000129",
        "tab.border": "#ff00958f",
        "sideBar.border": "#ff00958f",
        "activityBar.border": "#ff00958f",
        "statusBar.border": "#ff00958f",
        "editor.foreground": "#ffffff",
        "sideBar.foreground": "#ffffff",
        "activityBar.foreground": "#ffffff",
        "statusBar.foreground": "#ffe6b3",
        "tab.activeBackground": "#401837",
        "tab.unfocusedActiveBackground": "#550055",
        "tab.hoverBackground": "#55556A",
        "tab.inactiveBackground": "#191920",
        "editorLineNumber.foreground": "#ff66b3",
        "editorLineNumber.activeForeground": "#ff0095",
        "editorCursor.foreground": "#ff0095",
        "editor.selectionBackground": "#4444aa",
        "editor.selectionForeground": "#00ffbb95",
        "editor.selectionHighlightBorder": "#ff00958f",
        "input.background": "#111111",
        "input.border": "#ff00958f",
        "input.foreground": "#ffffff",
        "input.placeholderForeground": "#cc99ff",
        "focusBorder": "#ff00958f",
        "widget.border": "#ff00958f",
        "widget.shadow": "#444444",
        "selection.background": "#6134C3A6",
        "descriptionForeground": "#cc99ff",
        "errorForeground": "#ff3333",
        "icon.foreground": "#ffffff",
        "editorGroupHeader.border": "#ff00958f",
        "panel.border": "#ff00958f",
        "dropdown.border": "#ff00958f",
        "titleBar.border": "#ff00958f",
        "editor.findMatchBackground": "#ffd70088",
        "editor.findMatchHighlightBackground": "#ffd70066",
        "editorIndentGuide.background1": "#ffffff25",
        "editorIndentGuide.activeBackground1": "#ffffff40",
        "editorGutter.background": "#000010",
        "editorError.foreground": "#ff3333",
        "editorWarning.foreground": "#ffb733",
        "editorInfo.foreground": "#33ccff",
        "editorBracketMatch.background": "#ff009544",
        "editorBracketMatch.border": "#ff0095",
        "panel.background": "#000015",
        "panelTitle.activeBorder": "#ff0095",
        "panelTitle.activeForeground": "#ffffff",
        "panelTitle.inactiveForeground": "#cc99ff",
        "editorHoverWidget.border": "#ff00958f",
        "editorHoverWidget.background": "#000129",
        "activityBar.activeBorder": "#ff0095",
        "activityBar.activeBackground": "#401837",
        "sideBarSectionHeader.background": "#000129",
        "sideBarSectionHeader.border": "#ff00958f",
        "sash.hoverBorder": "#ff0095",
        "list.activeSelectionBackground": "#401837",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#55556A",
        "list.inactiveSelectionBackground": "#191920",
        "quickInput.background": "#000129",
        "quickInput.foreground": "#ffffff",
        "quickInputList.focusBackground": "#401837",
        "searchEditor.findMatchBorder": "#ff0095",
        "searchEditor.textInputBorder": "#ff0095",
        "editorGroup.border": "#ff0095"
    },
    "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#ff1e00e8"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#00ffb3"
            }
        },
        {
            "scope": ["constant.numeric", "constant.other.color.rgb-value", "constant.other.rgb-value", "support.constant.color"],
            "settings": {
                "foreground": "#00bfff"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#9370db"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ff1493"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#ff1493"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#ffa5A1"
            }
        },
        {
            "scope": ["source.css entity.other.attribute-name", "source.css.less entity.other.attribute-name.id", "source.scss entity.other.attribute-name"],
            "settings": {
                "foreground": "#ffa5A1"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#ff3333"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffd700"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#ff4500"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#ff8c00"
            }
        },
        {
            "scope": "support.type.property-name",
            "settings": {
                "foreground": "#87cefa"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ff00ff"
            }
        },
        {
            "name": "Function declarations",
            "scope": ["entity.name.function", "support.function", "support.constant.handlebars"],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "name": "Types declaration and references",
            "scope": ["meta.return-type", "support.class", "support.type", "entity.name.type", "entity.name.class"],
            "settings": {
                "foreground": "#ffd700"
            }
        },
        {
            "name": "Variable declarations",
            "scope": ["meta.definition.variable"],
            "settings": {
                "foreground": "#ff66cc"
            }
        },
        {
            "name": "Variable and parameter name",
            "scope": ["variable", "support.variable"],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Object keys, TS grammar specific",
            "scope": ["meta.object-literal.key", "meta.object-literal.key entity.name.function"],
            "settings": {
                "foreground": "#6674f8"
            }
        },
        {
            "name": "Array variables",
            "scope": [
                "meta.array.literal",
                "variable.other.array",
                "meta.sequence.list"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "Object variables",
            "scope": [
                "variable.other.object",
                "meta.object.member",
                "variable.other.class"
            ],
            "settings": {
                "foreground": "#f5a196"
            }
        },
        {
            "name": "String variables",
            "scope": [
                "variable.other.string",
                "variable.other.constant.string",
                "string.quoted.variable"
            ],
            "settings": {
                "foreground": "#FFB6C1"
            }
        },
        {
            "name": "Number variables",
            "scope": [
                "variable.other.number",
                "variable.other.constant.number",
                "constant.numeric.decimal.variable"
            ],
            "settings": {
                "foreground": "#87CEEB"
            }
        },
        {
            "name": "Boolean variables",
            "scope": [
                "variable.other.constant.boolean",
                "constant.language.boolean.variable"
            ],
            "settings": {
                "foreground": "#DDA0DD"
            }
        },
        {
            "name": "Regular Expression variables",
            "scope": [
                "variable.other.regexp",
                "string.regexp.variable"
            ],
            "settings": {
                "foreground": "#FFA07A"
            }
        },
        {
            "name": "Date/Time variables",
            "scope": [
                "variable.other.constant.date",
                "variable.other.constant.time"
            ],
            "settings": {
                "foreground": "#98FF98"
            }
        },
        {
            "name": "Function variables",
            "scope": [
                "variable.other.function",
                "entity.name.function.variable"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Promise/Future variables",
            "scope": [
                "variable.other.promise",
                "variable.other.future"
            ],
            "settings": {
                "foreground": "#E6E6FA"
            }
        },
        {
            "name": "Map/Dictionary variables",
            "scope": [
                "variable.other.map",
                "variable.other.dictionary"
            ],
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        {
            "name": "Set variables",
            "scope": [
                "variable.other.set",
                "meta.set.variable"
            ],
            "settings": {
                "foreground": "#FF7F50"
            }
        },
        {
            "name": "Buffer/Stream variables",
            "scope": [
                "variable.other.buffer",
                "variable.other.stream"
            ],
            "settings": {
                "foreground": "#7FFFD4"
            }
        },
        {
            "name": "Error/Exception variables",
            "scope": [
                "variable.other.error",
                "variable.other.exception"
            ],
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "name": "Iterator variables",
            "scope": [
                "variable.other.iterator",
                "variable.other.enumeration"
            ],
            "settings": {
                "foreground": "#BDB76B"
            }
        },
        {
            "name": "URL/URI variables",
            "scope": [
                "variable.other.url",
                "variable.other.uri"
            ],
            "settings": {
                "foreground": "#40E0D0"
            }
        },
        {
            "name": "General variables and parameters",
            "scope": ["variable", "support.variable"],
            "settings": {
                "foreground": "#00ffff"
            }
        },
    ]
}