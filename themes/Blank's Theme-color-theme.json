{
    "name": "Blank's Theme",
    "colors": {
        "editor.background": "#000010",
        "sideBar.background": "#000010",
        "activityBar.background": "#000129",
        "statusBar.background": "#000129",

        "tab.border": "#ff00958f",
        "sideBar.border": "#ff00958f",
        "activityBar.border": "#ff00958f",
        "statusBar.border": "#ff00958f",

        "editor.foreground": "#ffffff", // Text color in the editor
        "sideBar.foreground": "#ffffff", // Text color in the side bar
        "activityBar.foreground": "#ffffff", // Text color in the activity bar
        "statusBar.foreground": "#ffffff", // Text color in the status bar

        "tab.activeBackground": "#330033", // Active tab background
        "tab.unfocusedActiveBackground": "#440044", // Unfocused active tab background
        "tab.hoverBackground": "#550055", // Tab hover background
        "tab.inactiveBackground": "#660066", // Inactive tab background

        "editorLineNumber.foreground": "#666666", // Line number color
        "editorLineNumber.activeForeground": "#ffffff", // Active line number color
        "editorCursor.foreground": "#ff0000", // Cursor color
        "editor.selectionBackground": "#333333", // Selection background
        "editor.selectionHighlightBorder": "#666666", // Selection border

        "input.background": "#111111", // Input box background
        "input.border": "#ff00958f", // Input box border
        "input.foreground": "#ffffff", // Input box text color
        "input.placeholderForeground": "#666666", // Input box placeholder text color

        "focusBorder": "#ff0000", // Focus border color
        "widget.border": "#ff00958f", // Widget border color
        "widget.shadow": "#333333", // Widget shadow color

        "selection.background": "#5124B3A6", // Overall selection background
        "descriptionForeground": "#666666", // Description text color
        "errorForeground": "#ff0000", // Error text color
        "icon.foreground": "#ffffff" // Icon color
    },

    "tokenColors": [
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#ffee00cc"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#00ffb3"
            }
        },
        {
            "scope": ["constant.numeric", "constant.other.color.rgb-value", "constant.other.rgb-value", "support.constant.color"],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#5eff00"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#ffae00"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#00a6ff"
            }
        },
        {
            "scope": ["source.css entity.other.attribute-name", "source.css.less entity.other.attribute-name.id", "source.scss entity.other.attribute-name"],
            "settings": {
                "foreground": "#ffae00"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#8b0000"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#d7ba7d"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": ["punctuation.definition.tag"],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#d65656"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#bbff00"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#fd33f3"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#ff0095"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#ff7d49"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#e2bc8b"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#f292ff"
            }
        },
        {
            "name": "JavaScript string interpolation ${}",
            "scope": [
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.begin.ts",
                "punctuation.definition.template-expression.end.ts",
                "punctuation.definition.template-expression.end.js"
            ],
            "settings": {
                "foreground": "#d65a56"
            }
        },
        {
            "scope": ["support.type.vendored.property-name", "support.type.property-name", "variable.css", "variable.scss", "variable.other.less"],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#b67c00"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ebe99c"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": ["storage.modifier.import.java", "variable.language.wildcard.java", "storage.modifier.package.java"],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "coloring of the TS this",
            "scope": "variable.language.this",
            "settings": {
                "foreground": "#b8b8b8"
            }
        },
        {
            "name": "Function declarations",
            "scope": ["entity.name.function", "support.function", "support.constant.handlebars"],
            "settings": {
                "foreground": "#00d897"
            }
        },
        {
            "name": "Types declaration and references",
            "scope": ["meta.return-type", "support.class", "support.type", "entity.name.type", "entity.name.class", "source.cs storage.type"],
            "settings": {
                "foreground": "#9626ff"
            }
        },
        {
            "name": "Types declaration and references, TS grammar specific",
            "scope": [
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#58ffde"
            }
        },
        {
            "name": "Control flow keywords",
            "scope": "keyword.control",
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Variable and parameter name",
            "scope": ["variable", "meta.definition.variable.name", "support.variable"],
            "settings": {
                "foreground": "#c3fbff"
            }
        },
        {
            "name": "Object keys, TS grammar specific",
            "scope": ["meta.object-literal.key", "meta.object-literal.key entity.name.function"],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "CSS property value",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#ff7944"
            }
        },
        // Go
        {
            "scope": ["keyword.package.go", "keyword.import.go"],
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "scope": "storage.type.go",
            "settings": {
                "foreground": "#4ECDC4"
            }
        },
        {
            "scope": "entity.name.function.go",
            "settings": {
                "foreground": "#d881e0"
            }
        },

        // Rust
        {
            "scope": "keyword.other.fn.rust",
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "scope": "storage.type.rust",
            "settings": {
                "foreground": "#00b8ab"
            }
        },
        {
            "scope": "entity.name.function.rust",
            "settings": {
                "foreground": "#45B7D1"
            }
        },
        // JavaScript
        {
            "scope": "variable.other.object.js",
            "settings": {
                "foreground": "#0a65ce"
            }
        },
        {
            "scope": "support.class.js",
            "settings": {
                "foreground": "#FFDC00"
            }
        },
        {
            "scope": "keyword.operator.expression.typeof.js",
            "settings": {
                "foreground": "#B10DC9"
            }
        },

        // Python
        {
            "scope": "keyword.operator.logical.python",
            "settings": {
                "foreground": "#FF4136"
            }
        },
        {
            "scope": "support.type.python",
            "settings": {
                "foreground": "#3D9970"
            }
        },
        {
            "scope": "support.function.builtin.python",
            "settings": {
                "foreground": "#ddddcd"
            }
        },

        // C#
        {
            "scope": "keyword.other.using.cs",
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "scope": "storage.type.cs",
            "settings": {
                "foreground": "#4ECDC4"
            }
        },
        {
            "scope": "entity.name.type.class.cs",
            "settings": {
                "foreground": "#4ecae6"
            }
        },

        // Nested object keys
        {
            "scope": "meta.object-literal.key",
            "settings": {
                "foreground": "#FF851B"
            }
        },
        {
            "scope": "meta.object-literal.key meta.object-literal.key",
            "settings": {
                "foreground": "#FFDC00"
            }
        },
        {
            "scope": "meta.object-literal.key meta.object-literal.key meta.object-literal.key",
            "settings": {
                "foreground": "#39CCCC"
            }
        },
        {
            "scope": "meta.object-literal.key meta.object-literal.key meta.object-literal.key meta.object-literal.key",
            "settings": {
                "foreground": "#B10DC9"
            }
        }
    ]
}
